aurant => {\r\n        return (\r\n          restaurant.value.toLowerCase().indexOf(queryString.toLowerCase()) ===\r\n          0\r\n        );\r\n      };\r\n    },\r\n    loadAll() {\r\n      return [\r\n        { value: \"知乎\", path: \"\" },\r\n        {\r\n          value: \"后台模板\",\r\n          path: \"\"\r\n        },\r\n        {\r\n          value: \"开发文档\",\r\n          path: \"\"\r\n        },\r\n        { value: \"gitHub\", path: \"\" }\r\n      ];\r\n    },\r\n    handleSelect(item) {\r\n      console.log(item);\r\n    },\r\n    changeWebPath(ev) {\r\n      console.log(ev);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.fa {\r\n  width: 100vw;\r\n  height: 100vh;\r\n  /* background: url(\"../../assets/img/welBgImg.jpg\"); */\r\n  background-size: 100% 100%;\r\n  position: relative;\r\n}\r\n.serachInput {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/welcome"}]}