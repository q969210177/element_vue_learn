type=\"password\"\r\n              placeholder=\"请输入密码/PassWord\"\r\n              v-model=\"loginForm.passWord\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-row>\r\n              <el-col class=\"text_rigth\">\r\n                <el-button\r\n                  size=\"mini\"\r\n                  type=\"success\"\r\n                  @click=\"login('loginForm')\"\r\n                >\r\n                  <div>登录</div>\r\n                  <div>Login</div>\r\n                </el-button>\r\n                <el-button size=\"mini\">\r\n                  <div>取消</div>\r\n                  <div>cancal</div>\r\n                </el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Axios from \"axios\";\r\nimport qs from \"qs\";\r\nexport default {\r\n  data() {\r\n    //let userVeri = function(rule,value,callback) {};\r\n    return {\r\n      loginForm: {\r\n        userName: \"admin\",\r\n        passWord: \"admin\"\r\n      },\r\n      loginFormRules: {\r\n        userName: [\r\n          { required: true, message: \"请输入用户名\", trigger: \"blur\" },\r\n          { min: 4, max: 16, message: \"长度在 3 到 16 个字符\", trigger: \"blur\" }\r\n        ],\r\n        passWord: [\r\n          { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n          { min: 3, max: 16, message: \"长度在 3 到 16 个字符\", trigger: \"blur\" }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  mounted () {\r\n    \r\n  },\r\n  methods: {\r\n    login(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          console.log(this.loginForm.userName)\r\n          let user = qs.stringify({ user:this.loginForm.userName})\r\n          Axios({\r\n              method: 'post',\r\n              url:\"http://localhost//php/login/login.php\",\r\n              data:user\r\n          }).then((res)=>{\r\n            console.log(res.data);\r\n            if (res.data===404) {\r\n              this.$message({\r\n              message:\"没有注册的用户\",\r\n              type: 'warning'\r\n            });\r\n              \r\n            } else {\r\n              this.$message({\r\n              message:\"登录成功\",\r\n              type: 'success',\r\n              showClose:true,\r\n              onClose:function(vm){ \r\n                console.log(vm);\r\n                console.log(this);\r\n                \r\n                //this.$router.push({path:\"/home\"})\r\n               //vm.$router.push({path:\"/index\"})\r\n              }\r\n            });\r\n            }\r\n          })\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.login {\r\n  height: 100vh;\r\n  // background:url(\"../assets/img/login_bg.jpg\") no-repeat;\r\n  background-size: cover;\r\n}\r\n/deep/.el-input__suffix {\r\n  color: green;\r\n}\r\n.login_form{\r\npadding-top: 20%\r\n}\r\n</style>"]}]}